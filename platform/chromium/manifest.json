{
  "author": "Rafael Ketsetsides",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": ["browser-polyfill.min.js", "background.js"]
  },
  "description": "Metasearch is a search engine aggregator.",
  "manifest_version": 2,
  "name": "Metasearch",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "contextMenus",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "24": "img/icon24.png",
      "32": "img/icon32.png"
    },
    "default_title": "Metasearch"
  },
  "version": "1.3.6"
}
