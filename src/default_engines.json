    "baseurl": "https://stackoverflow.com/search?q={searchTerms}",
    "xpath_result": "//*[(contains(concat(' ',@class,' '), ' search-result '))]/div[2]",
    "xpath_title": ".//a[(contains(concat(' ',@class,' '), ' question-hyperlink '))]",
    "xpath_url": ".//a[(contains(concat(' ',@class,' '), ' question-hyperlink '))]",
    "xpath_summary": ".//div[(contains(@class, 'excerpt'))]",
    "timeout": 5000,
    "ttl": 86400000
  },
  {
    "id": "default_imdb",
