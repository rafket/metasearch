[
  {
    "id": "default_ddg",
    "name": "Duck Duck Go",
    "alias": "ddg",
    "keywords": "*",
    "baseurl": "https://duckduckgo.com/html/?q={searchTerms}",
    "xpath_result": "//*[contains(concat(' ',@class,' '), ' web-result ')]/div",
    "xpath_title": "./h2/a",
    "xpath_url": "./h2/a",
    "xpath_summary": "./a",
    "timeout": 5000,
    "ttl": 300000
  },
  {
    "id": "default_wiki",
    "name": "Wikipedia",
    "alias": "wiki",
    "keywords": "*",
    "baseurl": "https://en.wikipedia.org/w/index.php?title=Special:Search&fulltext=1&search={searchTerms}",
    "xpath_result": "//*[@id='mw-content-text']/div[3]/ul/li",
    "xpath_title": ".//div[1]/a",
    "xpath_url": ".//div[1]/a",
    "xpath_summary": ".//div[2]",
    "timeout": 5000,
    "ttl": 86400000
  },
  {
    "id": "default_so",
    "name": "Stack Overflow",
    "alias": "so",
